{
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^\\./\\../(.*)$",
        "^\\./(.*)$",
        "^[./]$"
    ],
    "importOrderCaseInsensitive": true,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": [
        "typescript",
        "decorators-legacy"
    ]
}